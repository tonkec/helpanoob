<% @posts.each do |p| %>

  <div class="col-xs-12 col-sm-12" style="border: 1px solid orange;">
    <p>
      Post: <%= link_to truncate(p.title, length: 300), post_path(p) %>
    </p>
    <p>
      Id: <%= p.id %>
    </p>

    <p>
      User: <%= link_to p.user.email, user_path(p.user) %>
    </p>

    <p>

    </p>

  </div>
<% end %>